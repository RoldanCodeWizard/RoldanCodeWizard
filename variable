# variable

my_string_variable = "my string variable"
print(my_string_variable)

my_string_variable = 12
print(my_string_variable)

my_int_str_variable = str("my_int_variable") 
print(my_int_str_variable)
print(type(my_int_str_variable))

my_bool_variable = True
print(my_bool_variable)

 # concatenacion de variable de es pring
print(type)(print(my_string_variable ,my_int_str_variable, my_bool_variable))
